{
  "name": "sam-cdk-vscode-debug",
  "version": "0.1.0",
  "bin": {
    "sam-cdk-vscode-debug": "bin/sam-cdk-vscode-debug.js"
  },
  "scripts": {
    "local-get": "sam-beta-cdk local invoke --profile marcio-dev SamCdkVscodeDebugStack/SamCdkVscodeDebugLambdaGet -e events/get.json --env-vars=env/env.json",
    "debug-get": "sam-beta-cdk local invoke --profile marcio-dev SamCdkVscodeDebugStack/SamCdkVscodeDebugLambdaGet -d 9999 -e events/get.json --env-vars=env/env.json",
    "local-post": "sam-beta-cdk local invoke --profile marcio-dev SamCdkVscodeDebugStack/SamCdkVscodeDebugLambdaPost -e events/post.json --env-vars=env/env.json",
    "debug-post": "sam-beta-cdk local invoke --profile marcio-dev SamCdkVscodeDebugStack/SamCdkVscodeDebugLambdaPost -d 9999 -e events/post.json --env-vars=env/env.json",
    "deploy": "cdk deploy -a .aws-sam/build --profile marcio-dev --require-aproval=never --all",
    "destroy": "cdk destroy -a .aws-sam/build --profile marcio-dev -f",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "coverage": "jest --coverage",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.129.0",
    "@types/aws-lambda": "^8.10.84",
    "@types/jest": "^26.0.24",
    "@types/node": "10.17.27",
    "aws-cdk": "1.129.0",
    "aws-sdk-mock": "^5.4.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.129.0",
    "@aws-cdk/aws-dynamodb": "^1.129.0",
    "@aws-cdk/aws-lambda": "^1.129.0",
    "@aws-cdk/aws-logs": "^1.129.0",
    "@aws-cdk/core": "1.129.0",
    "@aws-sdk/client-dynamodb": "^3.38.0",
    "@aws-sdk/util-dynamodb": "^3.38.0",
    "aws-sdk": "^2.1013.0",
    "aws-xray-sdk": "^3.3.3",
    "source-map-support": "^0.5.16"
  }
}
